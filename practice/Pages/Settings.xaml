<Page x:Class="practice.Pages.Settings"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:practice.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="800" d:DesignWidth="800"
      Title="Settings">

    <Grid>
        <Menu DockPanel.Dock="Left"  >
                <MenuItem x:Name="menuLanguage" Header="{DynamicResource text_menu_language}" />
                <MenuItem x:Name="menuTheme" Header="{DynamicResource text_menu_theme}" />
        </Menu>
    </Grid>
</Page>
